{
  "expo": {
    "name": "Target",
    "slug": "target",
    "scheme": "target",
    "version": "1.0.0",
    "runtimeVersion": "1.0.0",
    "platforms": ["android", "ios"],
    "owner": "edito",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "package": "com.editochokoso.target",
      "permissions": ["NOTIFICATIONS"],
      "edgeToEdgeEnabled": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#3D44CD"
      }
    },
    "ios": {
      "bundleIdentifier": "com.editochokoso.target",
      "icon": "./assets/ios-icon.png",
      "supportsTablet": true
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-sqlite",
      "expo-asset",
      [
        "expo-notifications",
        {
          "icon": "./assets/adaptive-icon.png",
          "color": "#3D44CD",
          "sounds": ["default"]
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b3c5646a-4d72-4441-8a31-b1ec31ad9ebb"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/b3c5646a-4d72-4441-8a31-b1ec31ad9ebb"
    }
  }
}
